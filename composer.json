{
    "name": "vietstars/client-detect",
    "description": "Desktop/mobile user agent parser with support for Laravel, based on Mobiledetect",
    "keywords": ["laravel", "useragent", "agent", "user agent", "browser", "platform", "mobile", "desktop"],
    "homepage": "https://github.com/vietstars/client-detect",
    "license" : "MIT",
    "authors": [
        {
            "name": "Vietstars",
            "email": "vietstar.nt@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "mobiledetect/mobiledetectlib": "^2.7.6",
        "jaybizzle/crawler-detect": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0|^6.0|^7.0",
        "php-coveralls/php-coveralls": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "Vietstars\\ClientDetect\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vietstars\\ClientDetect\\Test\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev"
        },
        "laravel": {
            "providers": [
                "Vietstars\\ClientDetect\\ClientServiceProvider"
            ],
            "aliases": {
                "Client": "Vietstars\\ClientDetect\\Facades\\Client"
            }
        }
    },
    "minimum-stability": "stable"
}
